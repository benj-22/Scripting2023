using System;
					
public class Program
{
	public static void Main()
	{
		int user = Convert.ToInt32(Console.ReadLine());//found this line on the internet. don't know how else to convert from string to int
		
		for (int i = 1; i <= user; i++){ //for determines how many times to run the loop
				
			for (int l = 1; l <= i; l++){
				
				Console.Write(i);
			
			}
			
			Console.WriteLine();
		}
	}
}
